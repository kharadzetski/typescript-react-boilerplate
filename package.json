{
  "name": "typescript2-react-webpack-boilerplate",
  "version": "1.0.0",
  "description": "TypeScript v2.0, React v14, Webpack v1.13 with hot reload boierplate",
  "main": "./app/index.html",
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "hot",
    "webpack-dev-server"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js --hot --inline",
    "dist": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js"
  },
  "author": "lieone",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^0.14.0",
    "@types/react-dom": "^0.14.0",
    "immutable": "3.8.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-hot-loader": "3.0.0-beta.6"
  },
  "devDependencies": {
    "cross-env": "4.0.0",
    "html-webpack-plugin": "2.24.1",
    "source-map-loader": "^0.1.5",
    "ts-loader": "2.0.3",
    "typescript": "^2.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
