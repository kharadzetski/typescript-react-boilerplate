{
  "name": "typescript2-react-webpack-boilerplate",
  "version": "1.2.0",
  "description": "TypeScript v2.0, React v14, Webpack v1.13 with hot reload boierplate",
  "main": "./app/index.html",
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "hot",
    "webpack-dev-server"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.renderer.config.js --inline",
    "predist": "npm run clean",
    "dist": "cross-env NODE_ENV=production webpack --config ./config/webpack.renderer.config.js --optimize-minimize"
  },
  "author": "lieone",
  "license": "MIT",
  "dependencies": {
    "@types/react": "15.0.34",
    "@types/react-dom": "15.5.1",
    "immutable": "3.8.1",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "1.3.1"
  },
  "devDependencies": {
    "awesome-typescript-loader": "3.2.1",
    "cross-env": "5.0.1",
    "html-webpack-plugin": "2.29.0",
    "rimraf": "2.6.1",
    "source-map-loader": "^0.1.5",
    "typescript": "2.4.1",
    "webpack": "3.0.0",
    "webpack-dev-server": "2.5.0"
  }
}
