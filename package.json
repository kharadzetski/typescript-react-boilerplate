{
  "name": "typescript-react-boilerplate",
  "version": "1.3.1",
  "description": "TypeScript v2, React v15, Webpack v3 with hot reload boierplate",
  "main": "./target/main.js",
  "author": {
    "name": "lieone",
    "email": "kstgone@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lieone/typescript-react-boilerplate.git"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "hot",
    "webpack-dev-server"
  ],
  "build": {
    "appId": "com.example.myapp",
    "files": [
      "./target/**/*"
    ]
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./target",
    "test": "echo \"Error: no test specified\" && exit 1",
    "main-dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.main.config.js -w",
    "main-dist": "cross-env NODE_ENV=production webpack --config ./config/webpack.main.config.js",
    "renderer-dist": "cross-env NODE_ENV=production webpack --config ./config/webpack.renderer.config.js --optimize-minimize",
    "renderer-dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.renderer.config.js --inline",
    "electron-dev": "cross-env NODE_ENV=development electron .",
    "electron-prod": "cross-env NODE_ENV=production electron .",
    "prepackage": "npm run clean && npm run main-dist && npm run renderer-dist",
    "package": "build",
    "start": "concurrently --kill-others \"npm run main-dev\" \"npm run renderer-dev\""
  },
  "license": "MIT",
  "dependencies": {
    "@types/react": "16.0.31",
    "@types/react-dom": "16.0.3",
    "immutable": "3.8.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "3.1.3"
  },
  "devDependencies": {
    "awesome-typescript-loader": "3.4.1",
    "concurrently": "3.5.1",
    "cross-env": "5.1.3",
    "electron": "1.7.10",
    "electron-builder": "19.49.2",
    "html-webpack-plugin": "2.30.1",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.3",
    "typescript": "2.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7"
  }
}
