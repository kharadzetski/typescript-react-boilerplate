{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Main Process",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "program": "${workspaceRoot}/target/main.js",
      "outFiles": [
        "${workspaceRoot}/target/*.js"
      ],
      "console": "internalConsole",
      "sourceMaps": true,
      "stopOnEntry": false,
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
